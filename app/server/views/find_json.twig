{% for user in users %}
<a href="/profile/{{ user._id }}">
<div class="col-sm-6 col-md-4 sort" data-age="{{ user.age }}" data-distance="{{ user.distance }}" data-popularite="{{ user.popularite }}">
<div class="thumbnail">
<div class="caption">

<h3> {{ user.user }} </h3>
<h4> popularite {{ user.popularite }} </h4>
<h6> {{ user.age }} </h6>
{{ user.sex }} <br>
{% if user.avatar %}
	<img src="{{ user.avatar }}" style="width: 100%">
{% endif %}
</div>
</div>
</div>
</a>
{% endfor %}
